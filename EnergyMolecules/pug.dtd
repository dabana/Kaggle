<!--
 (Input) Get a score matrix for a set of IDs; the result is a URL
 to a file containing uid-uid-score triplets.
-->
<!ELEMENT PCT-QueryScoreMatrix (
        PCT-QueryScoreMatrix_ids, 
        PCT-QueryScoreMatrix_ids-to?, 
        PCT-QueryScoreMatrix_type, 
        PCT-QueryScoreMatrix_format, 
        PCT-QueryScoreMatrix_compression?, 
        PCT-QueryScoreMatrix_n-3d-conformers?, 
        PCT-QueryScoreMatrix_no-3d-parent?)>

<!-- ID list; if ids-to not present, matrix will be ids X ids -->
<!ELEMENT PCT-QueryScoreMatrix_ids (PCT-QueryUids)>

<!-- If given, matrix will be ids X ids-to -->
<!ELEMENT PCT-QueryScoreMatrix_ids-to (PCT-QueryUids)>
<!-- Score type (more may be added in the future) -->
<!ELEMENT PCT-QueryScoreMatrix_type EMPTY>

<!--
    similarity-2d-subs	-  2D Tanimoto similarity based on substructure key fingerprint
    shape-optimized-3d	-  3D similarity optimized by shape
    feature-optimized-3d	-  3D similarity optimized by feature
-->
<!ATTLIST PCT-QueryScoreMatrix_type value (
        similarity-2d-subs |
        shape-optimized-3d |
        feature-optimized-3d
        ) #REQUIRED >

<!-- Format of the matrix output    -->
<!ELEMENT PCT-QueryScoreMatrix_format EMPTY>

<!--
    csv	-  Comma-separated values   
    id-id-score	-  Each line has id [tab] id [tab] score   
-->
<!ATTLIST PCT-QueryScoreMatrix_format value (
        csv |
        id-id-score
        ) #REQUIRED >

<!-- Compression of matrix output    -->
<!-- Default is gzip        -->
<!ELEMENT PCT-QueryScoreMatrix_compression EMPTY>

<!--
    none	-  no compression   
    gzip	-  gzip (.gz)   
    bzip2	-  bzip2 (.bz2)   
-->
<!ATTLIST PCT-QueryScoreMatrix_compression value (
        none |
        gzip |
        bzip2
        ) #REQUIRED >


<!--
 if 3D, use up to N conformers for each structure (if conformers are not being
 specified directly); not all records may have this many conformers, and upper limit 
 of N is set by the PubChem 3D system (currently 10)
 default 1
-->
<!ELEMENT PCT-QueryScoreMatrix_n-3d-conformers (#PCDATA)>

<!--
 by default, the 3D structure of a parent is used for scoring if the requested record has no 3D;
 set this to true to avoid this automatic substitution
 default false
-->
<!ELEMENT PCT-QueryScoreMatrix_no-3d-parent EMPTY>
<!ATTLIST PCT-QueryScoreMatrix_no-3d-parent value ( true | false ) #REQUIRED >


