<!ELEMENT PCT-ViewConformerAlignment_input-query (#PCDATA)>
<!-- Criteria for removing displayed superposition -->
<!ELEMENT PCT-ViewConformerAlignment_filter (
        PCT-ViewConformerAlignment_filter_shape-score, 
        PCT-ViewConformerAlignment_filter_feature-score, 
        PCT-ViewConformerAlignment_filter_rmself)>

<!-- Maximum shape score for displayed superposition -->
<!ELEMENT PCT-ViewConformerAlignment_filter_shape-score (#PCDATA)>

<!-- Maximum feature score for displayed superposition -->
<!ELEMENT PCT-ViewConformerAlignment_filter_feature-score (#PCDATA)>

<!-- Remove self neighboring pair to be displayed  -->
<!ELEMENT PCT-ViewConformerAlignment_filter_rmself EMPTY>
<!ATTLIST PCT-ViewConformerAlignment_filter_rmself value ( true | false ) #REQUIRED >

<!-- Conformer superposition type -->
<!ELEMENT PCT-ViewConformerAlignment_optimization (#PCDATA)>

<!--
    shape-based	-  Maximize overlap of atoms
    feature-based	-  Maximize overlap of features
-->
<!ATTLIST PCT-ViewConformerAlignment_optimization value (
        shape-based |
        feature-based
        ) #IMPLIED >

<!-- Conformer pair display order options -->
<!ELEMENT PCT-ViewConformerAlignment_sort (#PCDATA)>

<!--
    shape-then-feature	-  descending order of shape similarity then feature similarity
    feature-then-shape	-  descending order of feature similarity then shape similarity
